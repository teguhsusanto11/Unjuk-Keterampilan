<!-- views/products.ejs -->
<!DOCTYPE html>
<html>
<head>
    <title>Products</title>
</head>
<body>
    <h1>Product List</h1>
    <ul>
        <% products.forEach(product => { %>
            <li><%= product.name %> - <%= product.price %></li>
        <% }); %>
    </ul>
</body>
</html>
